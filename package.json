{
  "scripts": {
    "start": "parcel -p 3000",
    "build": "parcel build --public-url=/ats",
    "prebuild": "rm -rf dist",
    "deploy": "rsync -avz -e ssh ./dist/ root@geoff.zone:/var/www_projects/ats/",
    "repl": "ts-node --project tsconfig.repl.json ./bin/repl.ts",
    "eval": "ts-node --project tsconfig.repl.json ./bin/eval.ts",
    "test": "ava"
  },
  "source": "demo/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@codemirror/commands": "^6.3.2",
    "@codemirror/state": "^6.3.2",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.22.1",
    "@types/node": "^20.9.4",
    "codemirror": "^6.0.1",
    "copy-text-to-clipboard": "^3.2.0",
    "parcel": "^2.10.3",
    "preact": "^10.19.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.10.3",
    "ava": "^5.3.1",
    "tsx": "^4.6.1"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime",
    "preact/jsx-dev-runtime": "preact/jsx-runtime"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "ava": {
    "files": [
      "./src/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
